{
  "name": "celo-mexico",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "22.x"
  },
  "packageManager": "npm@10",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build --webpack",
    "vercel-build": "NODE_OPTIONS='--max-old-space-size=4096' prisma generate && NODE_OPTIONS='--max-old-space-size=4096' next build --webpack",
    "start": "next start",
    "lint": "eslint app components lib hooks config data --max-warnings=0",
    "format": "prettier --write .",
    "type-check": "prisma generate && tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "prisma:reset": "prisma migrate reset --force --skip-seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "coverage:check": "node scripts/coverage-check.js",
    "test:unit": "jest tests/unit",
    "test:components": "jest tests/components",
    "test:integration": "jest tests/integration",
    "test:database": "jest tests/database",
    "test:security": "jest tests/security",
    "test:performance": "jest tests/performance",
    "test:smoke": "jest tests/smoke",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:components && npm run test:integration && npm run test:database && npm run test:security",
    "test:ci": "npm run test:all && npm run test:coverage && npm run test:e2e",
    "test:migrations": "npm run prisma:reset && npm run prisma:migrate && npm run prisma:seed",
    "coverage:report": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit && npm run test:security",
    "pre-commit": "lint-staged",
    "pre-deploy": "npm run test:ci && npm run security:check && npm run build",
    "prepare": "husky install",
    "hardhat:compile": "hardhat compile",
    "hardhat:test": "hardhat test",
    "deploy:alfajores": "hardhat run scripts/deploy.ts --network alfajores",
    "deploy:celo": "hardhat run scripts/deploy.ts --network celo",
    "deploy:cmt-faucet:celo": "hardhat run scripts/deploy-cmt-faucet.cjs --network celo",
    "faucet:set-amount:celo": "hardhat run scripts/set-faucet-amount.cjs --network celo",
    "deploy:local": "hardhat run scripts/deploy.ts --network hardhat",
    "verify:alfajores": "hardhat verify --network alfajores",
    "verify:celo": "hardhat verify --network celo",
    "contracts:size": "hardhat size-contracts",
    "contracts:clean": "hardhat clean"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@prisma/client": "^5.22.0",
    "@privy-io/react-auth": "^2.16.0",
    "@privy-io/server-auth": "^1.32.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.29.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.87.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/sdk": "^5.4.40",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.10",
    "helmet": "^7.1.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.544.0",
    "next": "16.0.7",
    "next-axiom": "^1.0.0",
    "next-mdx-remote": "^6.0.0",
    "next-rate-limit": "^0.0.3",
    "next-themes": "^0.3.0",
    "permissionless": "^0.2.49",
    "pg": "^8.16.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "solc": "^0.8.30",
    "source-map": "^0.7.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.21.3",
    "wagmi": "^2.12.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/typography": "^0.5.18",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.5.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.12.0",
    "eslint-config-next": "^16.0.7",
    "hardhat": "^2.26.3",
    "hardhat-deploy": "^1.0.4",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^2.2.0",
    "lint-staged": "^15.0.0",
    "msw": "^2.0.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "type": "module"
}
